public class arrayDemo {
    public static void main(String[] args){
        int[] nums = new int[5];
        nums[0] = 8;
        nums[1] = 10;
        nums[2] = 12;
        nums[3] = 14;
        nums[4] = 16;

        for (int i = 0;i<5;i++){
            System.out.println(nums[i]);
        }

    }
}
